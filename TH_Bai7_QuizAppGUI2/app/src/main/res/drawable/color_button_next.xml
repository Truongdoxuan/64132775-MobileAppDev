<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape>
        <solid android:color="#40000000" />
        <corners android:radius="25dp" />
        <padding
            android:left="0dp"
            android:top="0dp"
            android:right="0dp"
            android:bottom="5dp" />
    </shape>
</item>
<item>
    <shape>
        <gradient
            android:startColor="#72ADB6"
            android:endColor="#366269"
            android:angle="40" />
        <corners android:radius="25dp" />
    </shape>
</item>
</layer-list>
